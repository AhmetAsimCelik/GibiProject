@using GibiProject.Entities
@model List<Dizi>
@{
    ViewBag.Title = "Index";
}
<div class=" row">
    @foreach (Dizi item in Model)
    {
        <div class="col-sm-3">


            <div class="card" style="width: 18rem;">
                <img src="~/Content/img/@item.DiziFoto" class="card-img-top" alt="..." width="100" height="300">
                <div class="card-body">
                    <h5 class="card-title">@item.DiziAdi</h5>
                    <p class="card-text">@item.Aciklama</p>
                    <a href="#" class="btn btn-primary">Bölümler</a>
                </div>
            </div>

        </div>
    }



</div>







